{
  "name": "elevator-root",
  "private": true,
  "scripts": {
    "compile": "turbo run compile",
    "build": "turbo run build",
    "start": "turbo run start",
    "web": "turbo run web",
    "ios": "turbo run ios",
    "android": "turbo run android",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "postinstall": "manypkg check"
  },
  "dependencies": {
    "@manypkg/cli": "latest",
    "eslint": "^8.38.0",
    "eslint-config-elevator": "workspace:^",
    "prettier": "^2.8.7",
    "turbo": "latest"
  },
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "resolutions": {
    "react": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  }
}
